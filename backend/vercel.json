{
    "version": 2,
    "builds": [
        {
            "src": "user_auth_django/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "user_auth_django/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "user_auth_django.settings"
    }
}